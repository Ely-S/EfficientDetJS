{
    "name": "efficientdet.js",
    "version": "0.0.0",
    "description": "A library to run EfficientDet in the browser",
    "main": "index.js",
    "files": [
        "build/*"
    ],
    "scripts": {
        "build": "tsc",
        "format": "prettier --write src/*.ts",
        "docs": "typedoc --out docs src",
        "lint": "tslint -p tsconfig.json",
        "test": "jest"
    },
    "preversion": "npm run lint",
    "homepage": "https://github.com/Ely-S/EfficientDetJS",
    "repository": {
        "type": "git",
        "url": "git@github.com:Ely-S/EfficientDetJS.git"
    },
    "keywords": [
        "tf.js",
        "tensorflow",
        "efficientdet",
        "object recognition"
    ],
    "author": "Ely-S",
    "license": "ISC",
    "devDependencies": {
        "@babel/preset-env": "^7.12.1",
        "@babel/preset-typescript": "^7.12.1",
        "@tensorflow/tfjs": "^2.5.0",
        "@types/jest": "^26.0.15",
        "jest": "^26.6.3",
        "prettier": "^2.1.2",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "typedoc": "^0.19.2",
        "typescript": "^4.0.5"
    },

   "jest": {
   	"verbose": true,
	"testPathIgnorePatterns": ["layers/", "/node_modules/"]
    }

}
